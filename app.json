{
  "expo": {
    "name": "SchengenTrack",
    "slug": "SchengenTrack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a5276"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.schengentrack.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a5276"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.schengentrack.app",
      "permissions": ["NOTIFICATIONS"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#1a5276"
        }
      ]
    ]
  }
}
